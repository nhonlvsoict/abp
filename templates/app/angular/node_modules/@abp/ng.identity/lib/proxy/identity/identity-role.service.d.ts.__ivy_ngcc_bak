import type { ListResultDto, PagedAndSortedResultRequestDto, PagedResultDto } from '@abp/ng.core';
import { RestService } from '@abp/ng.core';
import type { IdentityRoleCreateDto, IdentityRoleDto, IdentityRoleUpdateDto } from './models';
export declare class IdentityRoleService {
    private restService;
    apiName: string;
    create: (input: IdentityRoleCreateDto) => import("rxjs").Observable<IdentityRoleDto>;
    delete: (id: string) => import("rxjs").Observable<void>;
    get: (id: string) => import("rxjs").Observable<IdentityRoleDto>;
    getAllList: () => import("rxjs").Observable<ListResultDto<IdentityRoleDto>>;
    getList: (input: PagedAndSortedResultRequestDto) => import("rxjs").Observable<PagedResultDto<IdentityRoleDto>>;
    update: (id: string, input: IdentityRoleUpdateDto) => import("rxjs").Observable<IdentityRoleDto>;
    constructor(restService: RestService);
}
