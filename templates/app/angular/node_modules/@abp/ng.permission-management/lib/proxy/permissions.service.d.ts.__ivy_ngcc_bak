import type { GetPermissionListResultDto, UpdatePermissionsDto } from './models';
import { RestService } from '@abp/ng.core';
export declare class PermissionsService {
    private restService;
    apiName: string;
    get: (providerName: string, providerKey: string) => import("rxjs").Observable<GetPermissionListResultDto>;
    update: (providerName: string, providerKey: string, input: UpdatePermissionsDto) => import("rxjs").Observable<void>;
    constructor(restService: RestService);
}
